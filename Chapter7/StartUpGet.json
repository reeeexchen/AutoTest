[
  {
    "description": "模拟一个无参的GET请求",
    "request": {
      "uri": "/getDemo",
      "method": "GET"
    },
    "response": {
      "text": "这是一个无参的GET请求的响应",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "模拟一个有参的GET请求",
    "request": {
      "uri": "/getParam",
      "method": "GET",
      "queries": {
        "name": "REX",
        "age": "21"
      }
    },
    "response": {
      "text": "带参的GET请求响应",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]